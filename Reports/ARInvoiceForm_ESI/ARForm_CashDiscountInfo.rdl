    <DataSet Name="CashDiscountInfo">
      <Query>
        <DataSourceName>dsARForm</DataSourceName>
        <CommandText>
          ="SELECT  T1.Days, T1.[Percent], T1.GrossValue, T1.NetValue, T1.TaxValue
          FROM FICashDiscountInfo_" + Parameters!TableGuid.Value + " T1
          WHERE T1.InvoiceNum = '" + cstr(Parameters!Pm_INVCHEAD_InvoiceNum.Value) + "'"
        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
